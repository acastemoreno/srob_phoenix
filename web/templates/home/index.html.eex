    <div class="info">
      <h2>Srob - Latin America</h2>
      <p>Student Regional Operationg Board - Latin America</p>
    </div>
    <img id="gear-1" src="<%= static_path(@conn, "/images/engranaje1.png") %>">
    <img id="gear-2" src="<%= static_path(@conn, "/images/engranaje2.png") %>">
